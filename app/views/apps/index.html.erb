<h1 align="center">Listing All Apps</h1>

<div align="center">
<%= will_paginate @apps %>
<% @apps.each do |app| %>

<ul class="listing">

<div class="row">

<div class="well col-md-4 col-md-offset-4">

<li class="article-title">

<%= link_to "#{app.name}", app_path(app) %>

</li>


</div>

</div>

</ul>

<% end %>
<%= will_paginate @apps %>

</div>